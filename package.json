{
    "name": "@interverse/three-particles",
    "version": "1.2.0",
    "description": "AAA-quality GPU particle system for Three.js with Niagara/Unity VFX Graph-like features",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist",
        "prepublishOnly": "yarn clean && yarn build"
    },
    "keywords": [
        "three",
        "threejs",
        "particles",
        "gpu",
        "webgpu",
        "vfx",
        "visual-effects",
        "niagara",
        "vfx-graph"
