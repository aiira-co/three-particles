    "author": "Interverse Engine",
    "license": "MIT",
    "peerDependencies": {
        "three": ">=0.182.0"
    },
    "devDependencies": {
        "@types/three": "^0.182.0",
        "rimraf": "^5.0.0",
        "typescript": "^5.3.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aiira-co/three-particles.git"
    },
    "bugs": {
        "url": "https://github.com/aiira-co/three-particles/issues"
    },
    "homepage": "https://github.com/aiira-co/three-particles#readme"
}